#!/bin/bash
##############
# All the stuff that should autostart on the first startup of awesome
##############
echo screen
fix_screen.sh >> /dev/null
echo start synergy
syn_c.sh
echo ssa
. ssa
#swap caps and esc
echo esc move
setxkbmap -option "caps:swapescape"
#Screen
echo screen
screen -xR -S dropdown
